# Output for precomputed files for precomputed-worker.
NMCP_PRECOMPUTED_OUTPUT=

# Location of any secrets files for CloudVolumes package for mapped volume into precomputed-worker
NMCP_SECRETS_VOLUME=

# AAD Authentication
NMCP_AUTHENTICATION_CLIENT_ID=
