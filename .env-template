# Auth secret for internal services for env var in nmcp-api and precomputed-worker
NMCP_AUTH_CLIENT_ID=

# Location of ontology files for mapped volume into nmcp-api
NMCP_ONTOLOGY_LOCATION=

# CCFv3 file for mapped volume into nmcp-api.
